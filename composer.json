{
    "name": "marcohern/dimages",
    "license":"MIT",
    "description": "Upload images and download them in different densities and sizes",
    "authors": [
        {
            "name": "Marco Hernandez",
            "email": "marcohern@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "intervention/image": "^2.5"
    },
    "require-dev": {
      "intervention/image": "^2.5",
      "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
      "psr-4": {
        "Marcohern\\Dimages\\": "src/"
      },
      "files": [
          "src/Helpers.php"
      ]
    },
    "autoload-dev": {
      "psr-4": {
        "Tests\\": "tests/"
      }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Marcohern\\Dimages\\DimagesServiceProvider"
            ]
        }
    }
}
